{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "diagnostics.globals": [
    "vim"
  ],
  "workspace.library": [
    "$VIMRUNTIME",
    "$XDG_DATA_HOME/nvim/my-nvim/lazy",
    "${3rd}/luv/library"
  ],
  "Lua.workspace.library": [
    "/root/.config/nvim/my-nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/auto-session/lua",
    "/root/.local/share/nvim/my-nvim/lazy/blink.cmp/lua",
    "/root/.local/share/nvim/my-nvim/lazy/bufferline.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/flash.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/gitsigns.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/indent-blankline.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/lazy.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/lualine.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/mason-lspconfig.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/mason.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/mini.pairs/lua",
    "/root/.local/share/nvim/my-nvim/lazy/multicursor.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/noice.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/nui.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/nvim-lspconfig/lua",
    "/root/.local/share/nvim/my-nvim/lazy/nvim-notify/lua",
    "/root/.local/share/nvim/my-nvim/lazy/nvim-treesitter-context/lua",
    "/root/.local/share/nvim/my-nvim/lazy/nvim-treesitter/lua",
    "/root/.local/share/nvim/my-nvim/lazy/nvim-web-devicons/lua",
    "/root/.local/share/nvim/my-nvim/lazy/plenary.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/render-markdown.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/snacks.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/which-key.nvim/lua",
    "/root/.local/share/nvim/my-nvim/lazy/yazi.nvim/lua"
  ],
  "_pluginTypes": [
    {
      "module": "blink.cmp",
      "types": [
        "blink.cmp.Config"
      ],
      "file": "code/blink.lua"
    },
    {
      "module": "render-markdown",
      "types": [
        "render.md.UserConfig"
      ],
      "file": "code/render-markdown.lua"
    },
    {
      "types": [
        "PluginEntry[]"
      ],
      "file": "manager-ui/registry.lua"
    },
    {
      "module": "tokyonight",
      "types": [],
      "file": "ui/theme.lua"
    },
    {
      "module": "bufferline",
      "types": [
        "bufferline.Options"
      ],
      "file": "ui/bufferline.lua"
    },
    {
      "module": "noice",
      "types": [
        "NoiceConfig"
      ],
      "file": "ui/noice.lua"
    },
    {
      "module": "ibl",
      "types": [
        "ibl.config"
      ],
      "file": "ui/indent.lua"
    },
    {
      "types": [
        "snacks.Config",
        "snacks.dashboard.Item[]"
      ],
      "file": "snacks.lua"
    },
    {
      "types": [
        "LazySpec",
        "YaziConfig | {}"
      ],
      "file": "tools/yazi.lua"
    },
    {
      "module": "flash",
      "types": [
        "Flash.Config"
      ],
      "file": "tools/flash.lua"
    },
    {
      "module": "auto-session",
      "types": [
        "AutoSession.Config"
      ],
      "file": "tools/session.lua"
    }
  ]
}